{
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "bugs": {
    "url": "https://github.com/mlaursen/react-md/issues"
  },
  "scripts": {
    "build": "lerna run build --ignore examples --ignore doc-generator",
    "format": "npm-run-all -p \"format:ts\" \"format:tsx\"",
    "format:ts": "prettier --write packages/*/**/*.ts",
    "format:tsx": "prettier --write packages/*/**/*.tsx",
    "start": "cd packages/examples && npm run start",
    "dev": "npm-run-all -p \"watch\" \"start\"",
    "watch": "lerna run watch --ignore doc-generator --ignore examples",
    "docs": "lerna run docs --ignore examples --ignore doc-generator",
    "toc": "./packages/doc-generator/src/index.js --no-proptypes --no-sassdoc"
  },
  "homepage": "https://github.com/mlaursen/react-md#readme",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlaursen/react-md.git"
  },
  "devDependencies": {
    "commander": "^2.15.1",
    "lerna": "^2.11.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.7"
  }
}
