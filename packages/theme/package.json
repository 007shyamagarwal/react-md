{
  "name": "@react-md/theme",
  "version": "2.0.0-alpha-1",
  "description": "A package for working with react-md to use material design colors.",
  "main": "./dist/styles.css",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "docs": "doc-generator --no-proptypes",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "npm run styles",
    "styles": "copyfiles -u 1 \"src/**/*.scss\" dist",
    "prepublishOnly": "npm run build"
  },
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "copyfiles": "^2.0.0",
    "doc-generator": "^2.0.0-alpha-1",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2"
  }
}
