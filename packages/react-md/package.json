{
  "name": "react-md",
  "version": "2.0.0-alpha-1",
  "description": "Reusable React components implementing Material Design through Sass.",
  "main": "lib/index.js",
  "esnext:main": "es/index.js",
  "types": "types/index.d.ts",
  "repository": "https://github.com/mlaursen/react-md",
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "run-script": "cross-env BABEL_ENV=node babel-node --extensions '.ts'",
    "prebuild": "rimraf es lib dist",
    "compile": "babel src --extensions '.ts,.tsx,.js,.jsx' -s -d",
    "build": "npm-run-all -p \"build:modules\" \"build:commonjs\"",
    "build:modules": "cross-env NODE_ENV=development BABEL_ENV=es npm run compile -- es",
    "build:modules:watch": "npm run build:modules -- --watch",
    "build:commonjs": "cross-env NODE_ENV=development BABEL_ENV=commonjs npm run compile -- lib",
    "build:commonjs:watch": "npm run build:commonjs -- --watch",
    "lint": "tslint -p .",
    "lint:fix": "npm run lint -- --fix",
    "tsd": "tsc",
    "test": "cross-env BABEL_ENV=test jest src",
    "check-version": "npm run run-script -- src/scripts/checkVersion.ts",
    "prepublishOnly": "npm-run-all type-check test lint check-version build"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.6.1",
    "resize-observer-polyfill": "^1.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/node": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-react": "^7.0.0-beta.46",
    "@babel/preset-typescript": "^7.0.0-beta.46",
    "@types/classnames": "^2.2.3",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.3.12",
    "@types/react-dom": "^16.0.5",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "cross-env": "^5.1.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.4.3",
    "npm-run-all": "^4.1.2",
    "object.assign": "^4.1.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-test-renderer": "^16.3.2",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.4",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3"
  },
  "peerDependencies": {
    "react": ">= 16.3",
    "react-dom": ">= 16.3"
  }
}
