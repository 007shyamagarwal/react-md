{
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "outDir": "./test",
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "esModuleInterop": true,
    "declaration": false,
    "typeRoots": [
      "node_modules/@types/"
    ],
  },
  "exclude": [
    "node_modules"
  ]
}
