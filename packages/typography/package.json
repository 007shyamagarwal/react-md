{
  "name": "@react-md/typography",
  "version": "2.0.0-alpha-1",
  "description": "This package is for including typography for react-md.",
  "scripts": {
    "build": "npm-run-all -p \"styles\"",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "styles": "copyfiles -u 1 \"src/**/*.scss\" dist",
    "docs": "doc-generator --no-proptypes",
    "prepublishOnly": "npm run build && npm run docs"
  },
  "files": [
    "dist/*"
  ],
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "repository": "https://github.com/mlaursen/react-md",
  "license": "MIT",
  "devDependencies": {
    "copyfiles": "^2.0.0",
    "doc-generator": "^2.0.0-alpha-1",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2"
  }
}
