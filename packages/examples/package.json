{
  "name": "examples",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@react-md/button": "^2.0.0-alpha-1",
    "@react-md/elevation": "^2.0.0-alpha-1",
    "@react-md/theme": "^2.0.0-alpha-1",
    "@react-md/tooltip": "^2.0.0-alpha-1",
    "@react-md/transition": "^2.0.0-alpha-1",
    "@react-md/typography": "^2.0.0-alpha-1",
    "@react-md/utils": "^2.0.0-alpha-1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts-ts": "2.16.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && npm run build-css -- --watch --recursive",
    "start-js": "react-scripts-ts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^23.1.0",
    "@types/node": "^10.3.3",
    "@types/react": "^16.3.18",
    "@types/react-dom": "^16.0.6",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "typescript": "^2.9.2"
  }
}
