{
  "name": "@react-md/utils",
  "version": "2.0.0-alpha-1",
  "description": "General util functions for react-md",
  "scripts": {
    "build:commonjs": "tsc -p tsconfig.commonjs.json",
    "build:modules": "tsc -p tsconfig.modules.json",
    "build:definitions": "tsc -p tsconfig.modules.json -d --declarationDir ./types",
    "build": "npm-run-all -p \"build:commonjs\" \"build:modules\" \"build:definitions\"",
    "clean": "rimraf es lib types",
    "prebuild": "npm run clean",
    "test": "jest src",
    "watch": "npm-run-all -p :build:commonjs -- --watch\" \"build:modules -- --watch -d --declarationDir ./types\"",
    "docs": "doc-generator --no-proptypes --no-sassdoc",
    "watch": "npm-run-all -p \"build:modules -- --watch -d --declarationDir ./types\"",
    "prepublishOnly": "npm run build && npm run docs"
  },
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./types/index.d.ts",
  "files": [
    "es/*",
    "lib/*",
    "types/*"
  ],
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "repository": "https://github.com/mlaursen/react-md",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.6"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.4",
    "@types/jest": "^23.1.0",
    "@types/prop-types": "15.5.3",
    "@types/react": "^16.3.18",
    "@types/react-dom": "^16.0.6",
    "doc-generator": "^2.0.0-alpha-1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "jest": "^23.1.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-test-renderer": "^16.4.1",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.6",
    "ts-lint": "^4.5.1",
    "typescript": "^2.9.2"
  }
}
